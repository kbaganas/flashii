<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape
            android:shape="line">
            <stroke
                android:color="@color/seekBarRemain"
                android:width="8dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape
                android:shape="line">
                <solid android:color="@color/seekBarRemain" />
                <stroke
                    android:color="@color/blueText2"
                    android:width="8dp"/>
            </shape>
        </clip>
    </item>
</layer-list>